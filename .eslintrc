{
  "extends": ["airbnb", "plugin:jsx-a11y/recommended", "prettier"],
  "plugins": ["jsx-a11y", "prettier"],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": "off"
  },
  "ignorePatterns": [
    "reportWebVitals.js",
    "App.test.js",
    "node_modules/",
    "build/",
    "dist/",
    "*.min.js"
  ]
}
